{
    "name": "donkey-i18n",
    "version": "1.0.0",
    "description": "A typesafe, dead-simple, easily swappable i18n library for prototyping in typescript.",
    "keywords": [
        "typescript",
        "i18n"
    ],
    "homepage": "https://github.com/matthewyingtao/donkey-i18n",
    "bugs": {
        "url": "https://github.com/matthewyingtao/donkey-i18n/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/matthewyingtao/donkey-i18n.git"
    },
    "license": "MIT",
    "author": "Matthew Tao <matthew.yingtao@gmail.com> (matthewtao.com)",
    "type": "module",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "check-format": "prettier --check .",
        "ci": "pnpm build && pnpm check-format && pnpm check-exports",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.17.4",
        "prettier": "^3.5.3",
        "typescript": "^5.8.3"
    },
    "packageManager": "pnpm@10.9.0"
}
